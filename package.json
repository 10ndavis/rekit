{
  "name": "rekit",
  "version": "0.2.11",
  "description": "Boilerplate and tools for React, Redux and React-router based project.",
  "bin": {
    "rekit": "./bin/cli.js"
  },
  "scripts": {
    "start": "node ./tools/server.js",
    "build": "node ./tools/build.js",
    "test": "node ./test.js",
    "test:app": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha-webpack --webpack-config webpack.test.config.js \"test/**/*.test.js\"",
    "build:test": "node ./tools/build_test_server.js",
    "add:feature": "node ./tools/add_feature.js",
    "add:action": "node ./tools/add_action.js",
    "add:async-action": "node ./tools/add_async_action.js",
    "add:page": "node ./tools/add_page.js",
    "add:component": "node ./tools/add_component.js",
    "rm:feature": "node ./tools/rm_feature.js",
    "rm:action": "node ./tools/rm_action.js",
    "rm:async-action": "node ./tools/rm_async_action.js",
    "rm:page": "node ./tools/rm_page.js",
    "rm:component": "node ./tools/rm_component.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "babel-preset-stage-0"
    ],
    "plugins": [
      "lodash",
      [
        "module-resolver",
        {
          "alias": {
            "src": "./src",
            "features": "./src/features"
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "test.js"
    ],
    "exclude": ["_tmp", ".tmp"],
    "sourceMap": false,
    "instrument": true
  },
  "webpackDevServerPort": 6076,
  "buildTestServerPort": 6077,
  "dependencies": {
    "babel-core": "^6.13.2",
    "chai": "^3.5.0",
    "lodash": "^4.14.2",
    "memobind": "^0.5.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3",
    "shelljs": "^0.7.3",
    "style-loader": "^0.13.1",
    "superagent": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-lodash": "^3.2.6",
    "babel-plugin-module-resolver": "^2.1.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-import-resolver-babel-module": "^2.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash-webpack-plugin": "^0.9.4",
    "mocha": "^3.0.2",
    "mocha-webpack": "^0.6.0",
    "nyc": "^8.1.0",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dashboard": "0.0.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.3.3"
  }
}
